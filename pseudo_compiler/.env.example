# LLM-VL模型集成配置
# 复制此文件为.env并填入实际的API密钥

# OpenAI API配置（用于GPT-4 Vision）
LLM_VL_API_ENDPOINT=https://api.openai.com/v1/chat/completions
LLM_VL_API_KEY=your_openai_api_key_here

# 或者使用其他LLM-VL服务
# LLM_VL_API_ENDPOINT=https://api.anthropic.com/v1/messages
# LLM_VL_API_KEY=your_anthropic_api_key_here

# 模型配置
LLM_VL_MODEL=gpt-4-vision-preview
LLM_VL_MAX_TOKENS=2000
LLM_VL_TEMPERATURE=0.1

# 功能开关
ENABLE_SYNTAX_CHECKER=true
ENABLE_AUTO_CORRECTION=false
ENABLE_BATCH_ANALYSIS=true

# 性能配置
API_RATE_LIMIT_MS=1000
MAX_CODE_LENGTH=5000
CACHE_RESULTS=true